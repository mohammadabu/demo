<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_cash_request" model="ir.sequence">
            <field name="name">Cash request sequence</field>
            <field name="code">cash.request.sequence</field>
            <field name="prefix">PETTY/</field>
            <field name="suffix">/%(range_year)s</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>
