<odoo>
    <data>
        <record id="custom_invoice_tree_list" model="ir.ui.view">
            <field name="name">Custom Invoice Tree List</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_invoice_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='invoice_origin']" position="after">
                    <field name="ref" optional="show"/>
                </xpath>
                <xpath expr="//field[@name='amount_total_signed']" position="before">
                    <field name="amount_total" optional="show" string="Total(Actual total)"/>
                </xpath>
                <!-- <xpath expr="//field[@name='amount_total_signed']" position="before">
                    <field name="amount_total" optional="show" string="Total(Actual total)"/>
                </xpath> -->
            </field>
        </record>
    </data>
</odoo>   
