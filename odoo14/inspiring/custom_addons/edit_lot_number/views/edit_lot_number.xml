<odoo>
    <data>
        <record id="edit_lot_form_new_fields" model="ir.ui.view">
            <field name="name">lot.number.form.new.fields</field>
            <field name="model">stock.production.lot</field>
            <field name="inherit_id" ref="stock.view_production_lot_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='main_group']/group/field[@name='ref']" position="after">
                    <field name="rack_no" />
                    <field name="aisle_no" />
                </xpath>
            </field>
        </record>
    </data>
</odoo>