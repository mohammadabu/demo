<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Other Payment sequences -->
        <record id="sequence_other_payment" model="ir.sequence">
            <field name="name">Other Payments sequence</field>
            <field name="code">account.payment.other</field>
            <field name="prefix">Other/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>
